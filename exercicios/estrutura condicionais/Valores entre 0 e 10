Algoritmo "valores entre 0 e 10"
    Var c, v, t, s : Inteiro
    
Inicio
    t <- 0
    s <- 0
    para c de 1 ate 6 com passo 1 faca
         escreva("Digite um valor: ")
         leia(v)
         
         se(v >= 0) e (v <= 10) entao 
            t <- t + 1
           se (v % 2 <> 0) entao 
               s <- s + v
           FimSe 
         FimSe 
        
         
    fimpara 
    escreval("Entre 0 e 10 vc tem:", t)
    escreval("A soma dos nÃºmeros impares: ", s)

FimAlgoritmo
